struct A {
    val a: Int64
}


fun helloWorld(): Unit {
    puts("Hello world!")
}

fun modBy2(_ n: Int64): Int64 {
    return n % 2
}

fun modBy2Expr(_ n: Int64) = n % 2

fun main() {
    val s = A.init(a: 32)
    var a = 1
    val b = 2
    while a < 15 {
        if modBy2(a) == 0 {
            helloWorld()
        } else {
            puts("Welcome to Wiz!!")
        }
        a = a + if modBy2Expr(a) == 0 { 1 } else { 3 }
    }
    val c = a + b
    puts("...bye")
}
